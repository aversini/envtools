{
  "name": "envtools",
  "version": "2.0.14",
  "description": "Set of env tools for Mac/Linux",
  "author": "Arno Versini",
  "bin": {
    "envtools": "./bin/envtools.js"
  },
  "files": [
    "version",
    "shell",
    "bin",
    "dist",
    "data/i18n",
    "data/third"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aversini/envtools.git"
  },
  "bugs": {
    "url": "https://github.com/aversini/envtools/issues"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "license": "MIT",
  "preferGlobal": true,
  "scripts": {
    "clean": "rimraf dist coverage",
    "build:dist": "cross-env BABEL_ENV=production NODE_ENV=production babel src --out-dir dist --ignore \"src/__tests__\"",
    "build": "npm-run-all --serial clean lint build:dist",
    "lint": "eslint src --ext .js",
    "pretest": "npm run lint",
    "prettier": "prettier-eslint --write \"src/**/*.js\"",
    "prettier:file": "prettier-eslint --write",
    "test": "cross-env BABEL_DISABLE_CACHE=1 BABEL_ENV=test NODE_ENV=test jest",
    "test:coverage": "npm-run-all \"test -- --coverage\"",
    "test:watch": "npm-run-all \"test -- --watch\"",
    "watch": "npm run build:dist -- --watch"
  },
  "dependencies": {
    "async": "2.6.1",
    "chalk": "2.4.1",
    "decompress": "4.2.0",
    "download": "7.1.0",
    "execa": "0.10.0",
    "fedtools-commands": "0.0.54",
    "fedtools-config": "0.0.47",
    "fedtools-i18n": "0.0.17",
    "fedtools-logs": "0.1.42",
    "fedtools-utilities": "1.0.44",
    "fs-extra": "7.0.0",
    "fuse.js": "3.2.1",
    "glob": "7.1.2",
    "inquirer": "6.0.0",
    "lodash": "4.17.10",
    "macos-release": "2.0.0",
    "moment": "2.22.2",
    "node-notifier": "5.2.1",
    "onecolor": "3.0.5",
    "opn": "5.3.0",
    "optimist": "0.6.1",
    "os-name": "2.0.1",
    "remark-html": "7.0.0",
    "remark-parse": "5.0.0",
    "semver": "5.5.0",
    "shelljs": "0.8.2",
    "unified": "7.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.2.0",
    "cross-env": "^5.1.3",
    "eslint": "^4.15.0",
    "eslint-config-versini": "^1.0.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "grunt": "^1.0.1",
    "grunt-contrib-clean": "^1.1.0",
    "jest": "^22.1.4",
    "load-grunt-tasks": "^3.5.2",
    "merge": ">=1.2.1",
    "npm-run-all": "^4.1.2",
    "prettier-eslint-cli": "^4.7.0",
    "rimraf": "^2.6.2"
  },
  "keywords": [
    "envtools"
  ]
}
