{
  "name": "envtools",
  "version": "3.0.0",
  "description": "Set of env tools for Mac/Linux",
  "author": "Arno Versini",
  "bin": {
    "envtools": "./bin/envtools.js"
  },
  "files": [
    "version",
    "shell",
    "bin",
    "lib",
    "data/i18n",
    "data/third"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aversini/envtools.git"
  },
  "bugs": {
    "url": "https://github.com/aversini/envtools/issues"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "license": "MIT",
  "preferGlobal": true,
  "scripts": {
    "clean": "rimraf dist coverage",
    "build": "npm-run-all --serial clean lint",
    "lint": "eslint lib --ext .js",
    "pretest": "npm run lint",
    "prettier": "prettier-eslint --write \"lib/**/*.js\"",
    "prettier:file": "prettier-eslint --write",
    "test": "cross-env BABEL_DISABLE_CACHE=1 BABEL_ENV=test NODE_ENV=test jest",
    "test:coverage": "npm-run-all \"test -- --coverage\"",
    "test:watch": "npm-run-all \"test -- --watch\""
  },
  "dependencies": {
    "async": "2.6.1",
    "compression": "1.7.4",
    "decompress": "4.2.0",
    "download": "7.1.0",
    "execa": "0.10.0",
    "fedtools-commands": "0.0.54",
    "fedtools-config": "0.0.47",
    "fedtools-i18n": "0.0.17",
    "fedtools-logs": "0.1.42",
    "fedtools-utilities": "1.0.44",
    "fs-extra": "7.0.0",
    "fuse.js": "3.2.1",
    "glob": "7.1.2",
    "inquirer": "6.0.0",
    "kleur": "3.0.3",
    "lodash": "4.17.13",
    "macos-release": "2.0.0",
    "moment": "2.22.2",
    "node-notifier": "5.2.1",
    "onecolor": "3.0.5",
    "opn": "5.3.0",
    "optimist": "0.6.1",
    "os-name": "2.0.1",
    "portfinder": "1.0.25",
    "remark-html": "7.0.0",
    "remark-parse": "5.0.0",
    "semver": "5.5.0",
    "serve-handler": "6.1.2",
    "shelljs": "0.8.2",
    "unified": "7.0.0",
    "yargs": "15.0.2"
  },
  "devDependencies": {
    "cross-env": "^6.0.3",
    "eslint": "6.6.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-versini": "^1.0.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "grunt": "^1.0.4",
    "grunt-contrib-clean": "^2.0.0",
    "jest": "^24.9.0",
    "load-grunt-tasks": "^5.1.0",
    "merge": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "prettier-eslint-cli": "^5.0.0",
    "rimraf": "^3.0.0"
  },
  "keywords": [
    "envtools"
  ]
}
